{
    "name": "flagstudio/laravel-paginate",
    "description": "Different paginate macros for laravel eloquent builder and collection",
    "license": "MIT",
    "keywords": [
      "laravel-paginate-macros",
      "laravel-builder-macros",
      "laravel-collection-macros",
      "laravel-collection-paginate",
      "paginate-collection",
      "laravel-paginate-collection",
      "laravel-paginate-builder",
      "flagstudio"
    ],
    "authors": [
        {
            "name": "Vaishak Denis",
            "email": "vasishak.d@gmail.com",
            "homepage": "https://github.com/vasishakd"
        }
    ],
    "require": {
      "php": "^7.2",
      "laravel/framework": "^5.6.0|^5.7.0|^5.8.0|^6.0|^7.0"
    },
    "autoload": {
      "psr-4": {
        "Flagstudio\\PaginateMacros\\": "src"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Flagstudio\\PaginateMacros\\PaginateMacrosServiceProvider"
        ]
      }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
